<xml id="toolboxSetup" style="display: none">
    <category name="Struktur" colour="320">
        <block type="arduino_setup"></block>
        <block type="arduino_loop"></block>
    </category>

    <category name="Styring" colour="56">
        <block type="arduino_delay"></block>
      <block type="controls_if"></block>
      <block type="controls_if">
          <mutation else="1"></mutation>
      </block>
      <block type="controls_if">
          <mutation elseif="1" else="1"></mutation>
      </block>
      <block type="arduino_repeat"></block>
      <block type="arduino_for"></block>
    </category>

    <category name="Operatorer" colour="118">
        <block type="math_arithmetic"></block>
        <block type="logic_compare"></block>
        <block type="logic_operation"></block>
        <block type="logic_negate"></block>
        <block type="arduino_random"></block>
    </category>

    <category name="Maskinvare kontroll" colour="240">
        <block type="arduino_pinmode"></block>
        <block type="arduino_pinmode_variable"></block>
        <block type="arduino_blink"></block>
        <block type="arduino_digitalWrite"></block>
        <block type="arduino_digital_write_variable"></block>
        <block type="arduino_analog_write"></block>
        <block type="arduino_digital_read"></block>
        <block type="arduino_analog_read"></block>
        <block type="arduino_serial_begin"></block>
        <block type="arduino_serial_print"></block>
    </category>

    <category name="Funksjoner" colour="267">
        <!--block type="arduino_declare_function"></block>
        <block type="arduino_function"></block-->
        <block type="arduino_map"></block>
        <block type="arduino_tone_ms"></block>
        <block type="arduino_pulse_in"></block>
        <block type="arduino_raw_thermistor_to_temp_function"></block>
        <block type="arduino_raw_thermistor_to_temp"></block>
    </category>

    <category name="Variabler" colour="10">
        <block type="arduino_declare_variable"></block>
        <block type="arduino_declare_variable_with_value"></block>
        <block type="arduino_set_variable"></block>
        <block type="arduino_variable"></block>
        <block type="arduino_variable_value"></block>
        <block type="logic_boolean"></block>
    </category>

    <category name="Konstanter" colour="10">
        <block type="logic_null"></block>
        <block type="arduino_high_low"></block>
        <block type="arduino_digital_port"></block>
        <block type="arduino_analog_port"></block>
        <block type="arduino_tones"></block>
    </category>

    <category name="LED Pixels" colour="40">
      <block type="arduino_pixel_strip_include"></block>
      <block type="arduino_pixel_strip"></block>
      <block type="arduino_pixel_strip_begin"></block>
      <block type="arduino_pixel_strip_show"></block>
      <block type="arduino_pixel_strip_color"></block>
    </category>


    <category name="Sensorer" colour="40">
        <category name="Ultrasonic Sensor" colour="40">
            <block type="ultrasonic_init"></block>
            <block type="ultrasonic_setup"></block>
            <block type="ultrasonic_distance"></block>
        </category>
        <category name="DHT11 Temp+Fukt" colour="40">
            <block type="dht11_init"></block>
            <block type="dht11_setup"></block>
            <block type="dht11_read_temp"></block>
            <block type="dht11_read_humidity"></block>
        </category>
    </category>

    <category name="Motorer" colour="40">
        <category name="Arduino Bil L293/298" colour="40">
            <block type="arduino_car_variables"></block>
            <block type="arduino_car_setup"></block>
            <block type="arduino_car_forwards"></block>
            <block type="arduino_car_backwards"></block>
            <block type="arduino_car_stop"></block>
            <block type="arduino_car_rotate_right"></block>
            <block type="arduino_bil_rotate_left"></block>
            <block type="arduino_car_set_speed"></block>
            <block type="arduino_car_analog_speed"></block>
        </category>

        <category name="Servo Motor" colour="40">
            <block type="servo_include"></block>
            <block type="servo_attach"></block>
            <block type="servo_write"></block>
        </category>
    </category>

    <category name="Display" colour="40">
        <category name="OLED Display" colour="40">
            <block type="oled_include"></block>
            <block type="oled_setup"></block>
            <block type="oled_draw_pixel"></block>
            <block type="oled_set_cursor"></block>
            <block type="oled_print"></block>
            <block type="oled_clear_display"></block>
            <block type="oled_update_display"></block>
        </category>
    </category>

    <category name="EEPROM" colour="40">
        <block type="eeprom_include"></block>
        <block type="eeprom_write"></block>
        <block type="eeprom_update"></block>
        <block type="eeprom_read"></block>
        <block type="eeprom_length"></block>
    </category>
